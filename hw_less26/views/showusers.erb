



<table class="table table-striped">
  <thead>

  <tr>
      <th>Name</th>
      <th>phone</th>
      <th>datestamp</th>
      <th>barber</th>
      <th>color</th>
    </tr>
  </thead>
 <tbody>
    <% @users.each do |row| %>
    	 	<tr>
          <td><%=row['name'] %></td>
          <td><%=row['phone'] %></td>
          <td><%=row['datestamp'] %></td>
          <td><%=row['barber'] %></td>
          <td style="background-color: <%= row['color'] %>"><%=row['color'] %></td>
        </tr>
    <% end %>
  </tbody>
</table>