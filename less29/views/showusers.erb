



<table class="table table-striped">
  <thead>

  <tr>
      <th>Name</th>
      <th>phone</th>
      <th>datestamp</th>
      <th>barber</th>
      <th>color</th>
    </tr>
  </thead>
 <tbody>
    <% @clients.each do |client| %>
    	 	<tr>
          <td><%= client.name %></td>
          <td><%= client.phone %></td>
          <td><%= client.datestamp %></td>
          <td><%= client.barber %></td>
          <td style="background-color: <%= client.color %>"><%=client.color %></td>
        </tr>
    <% end %>
  </tbody>
</table>